          "Ref": "DefaultAcl"
        },
        "Egress": "false",
        "RuleAction": "Allow",
        "RuleNumber": "1010",
        "Protocol": "17",
        "CidrBlock": "0.0.0.0/0",
        "PortRange": {
          "From": "1024",
          "To": "65535"
        }
      }
    },
