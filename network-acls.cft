        "Egress": "false",
        "RuleAction": "Allow",
        "RuleNumber": "801",
        "Protocol": "6",
        "CidrBlock": "0.0.0.0/0",
        "PortRange": {
          "From": "443",
          "To": "443"
        }
      }
    },
    "AllowAllTcpEphemeral": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "NetworkAclId": {
          "Ref": "DefaultAcl"
        },
        "Egress": "false",
        "RuleAction": "Allow",
        "RuleNumber": "1000",
        "Protocol": "6",
        "CidrBlock": "0.0.0.0/0",
        "PortRange": {
